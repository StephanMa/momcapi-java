        <xs:annotation>
            <xs:appinfo source="EditVDU">
                <xrxe:label>
                    <xrx:i18n>
                        <xrx:key>xlink_href</xrx:key>
                        <xrx:default>href</xrx:default>
                    </xrx:i18n>
                </xrxe:label>
                <xrxe:relevant context="='dao' or $context='extref'"/>
            </xs:appinfo>
        </xs:annotation>
    </xs:attribute>
    <xs:attribute name="role" type="xs:string">
        <xs:annotation>
