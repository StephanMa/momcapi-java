        <xs:attribute ref="xlink:arcrole" use="optional"/>
        <xs:attribute ref="xlink:title" use="optional"/>
        <xs:attribute ref="xlink:show" use="optional"/>
        <xs:attribute ref="xlink:actuate" use="optional"/>
